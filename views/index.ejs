<!-- card sec start -->
<div class="home_products">

    <% if(typeof cards !== 'undefined') { %>
        <% for(card in cards) { %>

            <a class="cardLink" href="/show/<%=cards[card]._id%>" >
                <div class="card">
                    <div class="card_img">
                        <img src="<%= cards[card].url %>" alt="">
                    </div>

                    <div class="card_info">
                        <div> <%= cards[card].title.toUpperCase() %> </div>
                        <h5> Rs.<%= cards[card].Price %> </h5>
                    </div>
                </div>
            </a>
            
        <% } %>
    <% } %>
</div>
